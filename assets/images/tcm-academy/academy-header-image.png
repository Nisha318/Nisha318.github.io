<svg width="1200" height="630" viewBox="0 0 1200 630">
  <!-- Content from previous header image but at the correct social media dimensions -->
  <!-- Background -->
  <rect width="1200" height="630" fill="#0a192f"/>
  
  <!-- Grid Pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#64ffda" stroke-width="0.5" opacity="0.1"/>
    </pattern>
    
    <!-- Binary text pattern -->
    <pattern id="binary" width="200" height="200" patternUnits="userSpaceOnUse">
      <text x="10" y="20" fill="#64ffda" opacity="0.05" font-family="monospace" font-size="14">01001100</text>
      <text x="60" y="50" fill="#64ffda" opacity="0.05" font-family="monospace" font-size="14">01010100</text>
      <text x="110" y="80" fill="#64ffda" opacity="0.05" font-family="monospace" font-size="14">01001000</text>
    </pattern>
  </defs>
  
  <!-- Apply patterns -->
  <rect width="1200" height="630" fill="url(#grid)"/>
  <rect width="1200" height="630" fill="url(#binary)"/>
  
  <!-- Main Elements -->
  <g transform="translate(600, 315)">
    <!-- Shield -->
    <path d="M0,-100 L80,-60 L80,40 L0,80 L-80,40 L-80,-60 Z" 
          fill="#1e3a5f" stroke="#64ffda" stroke-width="3"/>
    
    <!-- Lock Icon -->
    <g transform="translate(0, -10)">
      <rect x="-20" y="-10" width="40" height="45" 
            rx="5" fill="none" stroke="#64ffda" stroke-width="3"/>
      <path d="M-12,-10 V-25 C-12,-40 12,-40 12,-25 V-10" 
            fill="none" stroke="#64ffda" stroke-width="3"/>
    </g>
  </g>
  
  <!-- Network Nodes -->
  <g fill="#64ffda" opacity="0.8">
    <circle cx="200" cy="100" r="4"/>
    <circle cx="1000" cy="500" r="4"/>
    <circle cx="300" cy="400" r="4"/>
    <circle cx="900" cy="200" r="4"/>
    <!-- Connection lines -->
    <line x1="200" y1="100" x2="300" y2="400" stroke="#64ffda" stroke-width="1" opacity="0.3"/>
    <line x1="900" y1="200" x2="1000" y2="500" stroke="#64ffda" stroke-width="1" opacity="0.3"/>
  </g>
</svg>